<div class="row">
  <div class="col s12 m12">
    <div class="main2 card blue-grey darken-1">
      <div class="card-content white-text">
        <% sleeping_times = [] %>
        <% @dates = [] %>
        <% @sleep_ids = [] %>
        <% @sleeps.each do |sleep| %>
        <% sleeping_time = ((sleep.wakeup_time - sleep.slept_time) / 3600.0) %>
        <% date = sleep.date.day %>
        <% sleep_id = sleep.id %>
        <% if sleeping_time < 0 %>
        <% sleeping_time = 24.0 + sleeping_time %>
        <% else %>
        <% sleeping_time = sleeping_time %>
        <% end %>
        <% sleeping_times << sleeping_time %>
        <% @dates << date %>
        <% @sleep_ids << sleep_id %>
        <% end %>
        <% @sleeping_times = sleeping_times.to_json.html_safe %>
        <%= render "chart2" %>
      </div>
    </div>
  </div>
</div>